{
	"name": "kratos-vscode",
	"displayName": "kratos-vscode",
	"description": "Debugger for kratos generators",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onDebug",
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"breakpoints": [
			{
				"language": "markdown"
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@types/socket.io-client": "^1.4.32",
		"@types/sqlite3": "^3.1.5",
		"socket.io-client": "^2.2.0",
		"sqlite3": "^4.1.0"
	}
}
